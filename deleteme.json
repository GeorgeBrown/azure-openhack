{
    "apiVersion":"apps/v1beta1",
    "kind":"Deployment",
    "metadata":{
        "name":"minecraft-rktzeluaf"
    },
    "spec":{
        "replicas":1,
        "template":{
            "metadata":{
                "labels":{
                    "app":"minecraft-rktzeluaf"
                }
            },
            "spec":{
                "volumes":null,
                "containers":[{
                    "name":"minecraft-rktzeluaf",
                    "image":"openhack/minecraft-server:2.0",
                    "env":[{
                        "name":"EULA",
                        "value":"TRUE"
                    }],
                    "ports":[{
                        "containerPort":25565,
                        "name":"minecraft"
                    },
                    {
                        "containerPort":25575,
                        "name":"microsoft"
                    }]
                }]
            }
        }
    }
}
